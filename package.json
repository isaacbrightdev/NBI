  "scripts": {
    "dev": "adastra dev",
    "build": "vite build",
    "deploy": "shopify theme push",
    "preview": "adastra preview",
    "lint": "eslint ./src/**/*.{js,jsx} --fix",
    "format": "prettier --write . --ignore-unknown",
    "dev:ipe": "dotenv -e .env.ipe-staging -- npm run dev -- -s=ipe-staging",
    "dev:nbi": "dotenv -e .env.nbi-staging -- npm run dev -- -s=nbi-staging",
    "dev:ipesems": "dotenv -e .env.ipe-production -- npm run dev -- -s=ipe-sems",
    "dev:nbisems": "dotenv -e .env.nbi-production -- npm run dev -- -s=nbi-sems"
  },
  "lint-staged": {
    "*.{liquid,json,yml}": "prettier --write",
    "./src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
