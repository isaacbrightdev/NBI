<header
  class=""
  id="main-header"
  data-short-nav="{{ section.settings.short_nav }}"
></header>

{% schema %}
{
  "name": "Header",
  "class": "sticky top-0 z-50",
  "settings": [
    {
      "type": "checkbox",
      "id": "short_nav",
      "label": "Enable Short nav",
      "default": false
    }
  ]
}
{% endschema %}
